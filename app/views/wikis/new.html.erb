<h2>Create a New Wiki</h2>
<div class="row">
  <div class='col-md-8'>
    <%= form_for @wiki do |f| %>
      <div class="form-group">
        <%= f.label :title %><br />
        <%= f.text_field :title, autofocus: true, class: 'form-control', placeholder: "Enter Wiki Name" %>
      </div>
      <div class="form-group">
        <%= f.label :body %><br />
        <%= f.text_area :body, class: 'form-control', placeholder: "Add information to your wiki" %>
      </div>
      <div class="form-group">
        <%= f.submit "Save Wiki", class: 'btn btn-success' %>
      </div>
    <% end %>
  </div>
</div>
