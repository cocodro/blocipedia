<h1><%= @wiki.title %></h1>
<p><%= @wiki.body %></p>
<div class="row">
  <%= link_to "Edit Wiki", edit_wiki_path(@wiki), class: 'btn btn-success' %>
  <%= link_to "Delete Wiki", @wiki, method: :delete, remote: true, class: 'btn btn-danger', data:{confirm: "Are you sure? The wiki will be permanently deleted."} %>
