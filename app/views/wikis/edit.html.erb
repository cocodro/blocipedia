<h2>Edit Wiki</h2>
  <div class="row">
    <div class='col-md-8'>
      <%= form_for @wiki do |f| %>
        <div class="form-group">
          <%= f.label :title %><br />
          <%= f.text_field :title, autofocus: true, class: 'form-control', placeholder: "Edit Wiki Name" %>
        </div>
        <div class="form-group">
          <%= f.submit "Save", class: 'btn btn-success' %>
        </div>
      <% end %>
    </div>
  </div>
<h2>Edit Sections</h2>
  <% @sections.each do |section| %>
    <%= render partial: 'sections/form', locals: {section: section} %>
  <% end %>
