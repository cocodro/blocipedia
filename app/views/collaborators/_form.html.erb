<%= form_tag wiki_collaborators_path(@wiki.id) do %>
  <div class="form-group">
    <%= label_tag :email %><br />
    <%= email_field_tag :email, nil, placeholder: "Enter New Collaborator's Email" %>
  </div>
  <div class="form-group">
    <%= submit_tag "Add Collaborator", class: 'btn btn-success' %>
  </div>
<% end %>
